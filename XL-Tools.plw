#--------------------------#
sub downloadIINDB
#--------------------------#
{
  # Local variables
  my $localIINDB = shift; # Default location
  # Show progress window
  $win->ChangeCursor($HOURGLASS);
  $winPb->Text($STR{'downloadIINDB'});
  $winPb->lblPbCurr->Text($STR{'connecting'}.' le-tools.com...');
  $winPb->lblCount->Text("0 %");
  $winPb->pbWinPb->SetRange(0, 100);
  $winPb->pbWinPb->SetPos(0);
  $winPb->pbWinPb->SetStep(1);
  $winPb->Center($win);
  $winPb->Show();
  # Start an agent
  my $ua = new LWP::UserAgent;
  $ua->agent($CONFIG{'USERAGENT'});
  $ua->default_header('Accept-Language' => 'en');
  # Check size of the remote file
  my $req    = new HTTP::Request HEAD => $IINDB_URL;
  my $res    = $ua->request($req);
  my $return = $res->status_line;
  if ($res->code == 200) { $TOTAL_SIZE = $res->header('content_length'); }
  else {
    # Hide progress window
    $win->ChangeCursor($ARROW);
    $winPb->lblPbCurr->Text('');
    $winPb->lblCount->Text('');
    $winPb->pbWinPb->SetPos(0);
    $winPb->Hide();
    return("$STR{'errorMsg'}: $STR{'errorConRemote'}...");
  }
  # Download the file
  if ($TOTAL_SIZE) {
    my $downloadData;
    # Download the file
    $winPb->lblPbCurr->Text($STR{'downloadIINDB'}.'...');
    my $response = $ua->get($IINDB_URL, ':content_cb' => sub {
      # Local variables
      my ($data, $response, $protocol) = @_;
      $downloadData       .= $data;                                     # Downloaded data
      my $totalDownloaded  = length($downloadData);                     # Size of downloaded data
      my $completed        = int($totalDownloaded / $TOTAL_SIZE * 100); # Pourcentage of download completed
      $winPb->pbWinPb->SetPos($completed);    # Set the progress bar
      $winPb->lblCount->Text("$completed %"); # Indicate purcentage
    });
    # Save data in a temp file
    my $IINDB_ZIP = $localIINDB."\.zip";
    if ($response and $response->is_success) {
      open(ZIP,">$IINDB_ZIP");
      binmode(ZIP);
      print ZIP $downloadData;
      close(ZIP);
    } else {
      # Hide progress window
      $win->ChangeCursor($ARROW);
      $winPb->lblPbCurr->Text('');
      $winPb->lblCount->Text('');
      $winPb->pbWinPb->SetPos(0);
      $winPb->Hide();
      return("$STR{'errorMsg'}: $STR{'errorConRemote'}...");
    }
    # Uncompress IINDB ZIP
    my ($error, $msg);
    if (-e $IINDB_ZIP) {
      $TOTAL_SIZE = 0;
      if (unzip $IINDB_ZIP => $localIINDB, BinModeOut => 1) {
        if (&validIINDB($localIINDB)) {
          unlink $IINDB_ZIP;
          $winConfig->tfIINDB->Text($localIINDB);
          $CONFIG{'IIN_DB_FILE'} = $localIINDB;
          &saveConfig(\%CONFIG);
        } else { $error = 1; $msg = "$STR{'errorMsg'}: $STR{'invalidFile'}"; }
      } else { $error = 1; $msg = "$STR{'errorMsg'}: $UnzipError"; }
    } else { $error = 1; $msg = "$STR{'errorMsg'}: $STR{'errorDownload'}..."; }
    # Turn off progress bar
    $win->ChangeCursor($ARROW);
    $winPb->lblPbCurr->Text('');
    $winPb->lblCount->Text('');
    $winPb->pbWinPb->SetPos(0);
    $winPb->Hide();
    # Final message
    if ($error) { return($msg); } # Error
    else        { return(0);    }
  } else {
    # Hide progress window
    $win->ChangeCursor($ARROW);
    $winPb->lblPbCurr->Text('');
    $winPb->lblCount->Text('');
    $winPb->pbWinPb->SetPos(0);
    $winPb->Hide();
    return("$STR{'errorMsg'}: $STR{'errorDownload'}...");
  }

}  #--- End downloadIINDB

