          while ($end   =~ /:$/) { chop($end);   }
          while ($start !~ /(?:[0-9a-fA-F]{1,4}\:){7}[0-9a-fA-F]{1,4}/) { $start .= ':0000'; }
          while ($end   !~ /(?:[0-9a-fA-F]{1,4}\:){7}[0-9a-fA-F]{1,4}/) { $end   .= ':ffff'; }
          if ($start and $end) { $newItem = "$start - $end"; }
